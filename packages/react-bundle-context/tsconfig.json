{
  "compilerOptions": {
    "jsx": "react-jsx",
    "module": "esnext",
    "target": "es5",
    "moduleResolution": "node",
    "lib": ["dom", "es2015"],
    "declaration": true,
    "strict": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "skipLibCheck": true
  }
}
